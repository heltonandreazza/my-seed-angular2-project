        "html-loader": "^0.4.5",
        "html-webpack-plugin": "^2.29.0",
        "lite-server": "^2.3.0",
        "raw-loader": "^0.5.1",
        "typescript": "^2.3.4",
        "version-bump-prompt": "^3.1.2",
        "webpack": "^3.0.0",
