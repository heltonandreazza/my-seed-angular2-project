    "description": "seed-project",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "webpack-dev-server --inline --progress --port 8080 --config webpack.config.dev.js",
        "build:prod": "del-cli dist && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli src/app/**/*.js src/app/**/*.ngfactory.ts src/app/**/*.js.map src/app/**/*.shim.ts src/app/**/*.ngsummary.json src/app/**/*.ngstyle.ts dist/app",
        "serve": "lite-server",
        "patch": "bump --patch --commit --tag",
        "minor": "bump --minor --commit --tag",
        "major": "bump --major --commit --tag",
        "pre-release": "bump --prerelease --commit --tag"
    },
    "author": "helton.andreazza",
    "license": "ISC",
