        "@angular/forms": "^4.2.5",
        "@angular/http": "^4.2.5",
        "@angular/platform-browser": "^4.2.5",
        "@angular/platform-browser-dynamic": "^4.2.5",
        "@angular/platform-server": "^4.2.5",
        "@angular/router": "^4.2.5",
        "@angular/upgrade": "^4.2.5",
        "core-js": "^2.4.1",
        "rxjs": "^5.4.1",
        "zone.js": "^0.8.12"
